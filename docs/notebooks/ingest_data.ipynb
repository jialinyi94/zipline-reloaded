   "source": [
    "from zipline.data.bundles import register, ingest\n",
    "from zipline.data.bundles.quandl import quandl_bundle\n",
    "\n",
    "# 注册 Quandl bundle\n",
    "register(\n",
    "    'quandl',\n",
    "    quandl_bundle,\n",
    "    calendar_name='NYSE',\n",
    ")\n",
    "\n",
    "# 执行 ingest\n",
    "ingest('quandl')"
   ]
  }
